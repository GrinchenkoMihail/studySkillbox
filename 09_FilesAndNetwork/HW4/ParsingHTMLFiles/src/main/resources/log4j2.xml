<?xml version="1.0" encoding="UTF-8"?>
<configuration status="WARN">
    <appenders>

        <File name="SavedImagesFile" fileName="logs/SavedImages.log">
            <PatternLayout pattern="%msg%n"/>
            <Filters>
                <MarkerFilter marker="SAVED_IMAGES"/>
            </Filters>
        </File>
    </appenders>

    <loggers>
        <root level="info">
            <appender-ref ref="SavedImagesFile" level = "info" />
        </root>
    </loggers>
</configuration>